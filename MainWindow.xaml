<Window x:Class="GPUImageViewer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GPUImageViewer"
        mc:Ignorable="d"
        Title="BigTiffViewer"
        Drop="Window_Drop" AllowDrop="True"
        WindowState="Maximized"
        MouseMove="image_MouseMove" PreviewMouseWheel="image_MouseWheel"
        MouseLeftButtonDown="image_MouseLeftButtonDown" MouseLeftButtonUp="image_MouseLeftButtonUp">
    <Grid x:Name="grid" Background="White">
        <Image x:Name="image" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"/>
    </Grid>
</Window>
